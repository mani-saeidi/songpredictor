   "source": [
    "spotify['Genre'] = pd.Categorical(spotify['Genre'])\n",
    "spotify['Genre'] = spotify['Genre'].cat.codes\n",
    "spotify['Length'] = spotify['Length'].str.replace(',', '')\n",
